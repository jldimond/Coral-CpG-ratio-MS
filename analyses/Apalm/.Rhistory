#Read in CpG data
Apalm_cpg<- read.delim("ID_CpG", header=FALSE)
Apalm_diff<- read.delim("day1and2temp_CpG", header = FALSE)
#Create density
Apalm_dencpg<-density(Apalm_cpg$V2, na.rm=T)
